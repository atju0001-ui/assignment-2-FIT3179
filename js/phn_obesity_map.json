{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 820,
  "height": 560,
  "title": "Adults Overweight or Obese (%), Australia â€” AIHW Table S9 (2022)",
  "projection": { "type": "equalEarth" },

  "layer": [
    {
      "data": {
        "url": "data/ne_110m.topojson",
        "format": { "type": "topojson", "feature": "ne_110m_admin_0_countries" }
      },
      "transform": [
        { "filter": "lower(datum.properties.name) == 'australia'" }
      ],
      "mark": {
        "type": "geoshape",
        "fill": "#f2f2f2",
        "stroke": "#888",
        "strokeWidth": 0.7
      }
    },
    {
      "data": {
        "url": "data/aihw_s9_phn_obesity_2022.csv",
        "format": { "type": "csv", "delimiter": ";" }
      },
      "mark": "bar",
      "encoding": {
        "x": {
          "field": "state",
          "type": "nominal",
          "title": "State/Territory"
        },
        "y": {
          "field": "age_std_pct",
          "type": "quantitative",
          "title": "Age-standardised (%)"
        },
        "color": {
          "field": "state",
          "type": "nominal",
          "title": "State"
        },
        "tooltip": [
          { "field": "phn_name", "title": "PHN Area" },
          { "field": "state", "title": "State" },
          { "field": "age_std_pct", "title": "Age-standardised (%)" },
          { "field": "crude_pct", "title": "Crude (%)" }
        ]
      }
    }
  ]
}
